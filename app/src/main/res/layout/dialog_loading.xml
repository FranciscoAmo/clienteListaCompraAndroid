<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="62dp"
        android:layout_height="68dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/animacionCircular"
        app:layout_constraintEnd_toEndOf="@+id/animacionCircular"
        app:layout_constraintStart_toStartOf="@+id/animacionCircular"
        app:layout_constraintTop_toTopOf="@+id/animacionCircular"
        app:srcCompat="@mipmap/carritolleno_foreground" />

    <ImageView
        android:id="@+id/animacionCircular"
        android:layout_width="110dp"
        android:layout_height="102dp"
        android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/change" />
</androidx.constraintlayout.widget.ConstraintLayout>