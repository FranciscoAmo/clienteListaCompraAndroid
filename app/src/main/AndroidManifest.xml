<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.francisco.listadelacompra">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/carritolleno"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProductsByType"></activity>
        <activity android:name=".TypeProducts" />
        <activity
            android:name=".dialogs.PopUpActionsProducts"
            android:theme="@style/AppTheme.popupStyles" />
        <activity android:name=".dialogs.DialogAddUserToList" />
        <activity android:name=".ListDetail" />
        <activity android:name=".Register" />
        <activity android:name=".ListProducts" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>